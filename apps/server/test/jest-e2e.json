{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testRegex": "/test/.*\\.(e2e-test|e2e-spec).(ts|tsx|js)$",
  "collectCoverageFrom" : ["test/**/*.{js,jsx,tsx,ts}", "!**/node_modules/**", "!**/vendor/**"],
  "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
  },
  "testTimeout" : 30000

}
