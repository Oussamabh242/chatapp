{
  "compilerOptions": {
    "esModuleInterop": true,
    "outDir":"./dist",
    "baseUrl":"./",
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization":false

  },
  "exclude": ["node_modules"]
}
